<h1><%= link_to @deck.name, "http://www.mtgtop8.com/#{@deck.url}" %></h1>
<h2>Main Deck</h2>
<ul>
  <% @deck.main_deck.each do |card| %>
    <li><%= link_to card.name, card_path(card) %> </li>
  <% end %>
</ul>

<h2>Sideboard</h2>
<ul>
  <% @deck.sideboard.each do |card| %>
    <li><%= link_to card.name, card_path(card) %> </li>
  <% end %>
</ul>

